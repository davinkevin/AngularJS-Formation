{
  "name": "Bet-Manager",
  "version": "1.0.0",
  "dependencies": {
    "angular": "1.4.4",
    "angular-messages": "1.4.4",
    "angular-resource": "1.4.4",
    "angular-route": "1.4.4",
    "bootstrap": "~3.3.5",
    "restangular": "~1.5.1",
    "lodash" : "3.10.1"
  },
  "overrides": {
    "jquery": {
      "main": {
        "dev": "",
        "prod": ""
      }
    },
    "angular": {
      "main": {
        "dev": "angular.js",
        "prod": [
          "angular.min.js",
          "angular.min.js.map"
        ]
      }
    },
    "angular-messages": {
      "main": {
        "dev": "angular-messages.js",
        "prod": [
          "angular-messages.min.js",
          "angular-messages.min.js.map"
        ]
      }
    },
    "bootstrap": {
      "main": {
        "dev": [
          "./dist/css/bootstrap.css",
          "./dist/fonts/glyphicons-halflings-regular.eot",
          "./dist/fonts/glyphicons-halflings-regular.svg",
          "./dist/fonts/glyphicons-halflings-regular.ttf",
          "./dist/fonts/glyphicons-halflings-regular.woff",
          "./dist/fonts/glyphicons-halflings-regular.woff2"
        ],
        "prod": [
          "./dist/css/bootstrap.min.css",
          "./dist/css/bootstrap.css.map",
          "./dist/fonts/glyphicons-halflings-regular.eot",
          "./dist/fonts/glyphicons-halflings-regular.svg",
          "./dist/fonts/glyphicons-halflings-regular.ttf",
          "./dist/fonts/glyphicons-halflings-regular.woff",
          "./dist/fonts/glyphicons-halflings-regular.woff2"
        ]
      }
    },
    "lodash": {
      "main": {
        "dev": "lodash.js",
        "prod": "lodash.min.js"
      }
    },
    "restangular": {
      "main": {
        "dev": "dist/restangular.js",
        "prod": "dist/restangular.min.js"
      }
    }
  }
}
